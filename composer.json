{
    "name": "cakedc/enum",
    "description": "Enumeration list for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "cakedc",
        "enum"
    ],
    "homepage": "https://github.com/cakedc/enum",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cakedc/enum/issues",
        "source": "https://github.com/cakedc/enum"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0",
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Enum\\Test\\": "tests"
        }
    }
}
